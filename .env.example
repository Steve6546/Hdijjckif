# API Keys
# Get your OpenRouter API key from https://openrouter.ai/keys
# This key allows access to multiple AI models through a single API
OPENROUTER_API_KEY=sk-or-v1-your-api-key-here

# Site information (sent with OpenRouter API requests)
SITE_URL=https://your-site-url.com
SITE_NAME=AI Brain Orchestration

# Authentication
SECRET_KEY=your_secret_key_here
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Database
SQLITE_DB_PATH=logs/activity.db

# Redis Cache
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0

# Server
HOST=0.0.0.0
PORT=8000

# GitHub Integration (Optional)
GITHUB_TOKEN=your_github_token_here
GITHUB_REPO=your_repo_name_here

# Restart Command (Optional)
RESTART_COMMAND=replit restart
